{
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "python.analysis.ignore": ["gage/_vendor", "tests"],
  "python.analysis.typeCheckingMode": "basic",
  "python.analysis.diagnosticSeverityOverrides": {
    "reportUntypedFunctionDecorator": "none",
    "reportUnusedImport": "warning",
    "reportGeneralTypeIssues": "warning",
    "reportUnknownParameterType": "warning",
    "reportMissingParameterType": "warning",
    "reportWildcardImportFromLibrary": "none"
  },
  "black-formatter.args": ["--skip-string-normalization"],
  "json.schemas": [
    {
      "fileMatch": ["gage.json"],
      "url": "/gage/gagefile.schema.json"
    }
  ],
  "files.associations": {
    "gage.json": "jsonc"
  },
  "cSpell.enabled": true,
  "cSpell.words": [
    "autoprint",
    "chardet",
    "CPUS",
    "creationflags",
    "dvcfile",
    "ECONNREFUSED",
    "EEXIST",
    "endswith",
    "ENOENT",
    "ESRCH",
    "findl",
    "followlinks",
    "funs",
    "gagefile",
    "gaierror",
    "gethostname",
    "getpgid",
    "importlib",
    "indexlib",
    "jschon",
    "killpg",
    "libcublas",
    "lineterm",
    "loglib",
    "mklink",
    "mktempdir",
    "natsort",
    "natsorted",
    "normlf",
    "opdef",
    "oplib",
    "opref",
    "opspec",
    "pkgdir",
    "preexec",
    "printl",
    "procs",
    "psutil",
    "pwsh",
    "relpath",
    "repls",
    "rmtempdir",
    "runfiles",
    "setsid",
    "SIGKILL",
    "SIGTERM",
    "skopt",
    "sourcecode",
    "subschema",
    "topdown",
    "unmerge",
    "utcformat",
    "xonsh"
  ]
}
